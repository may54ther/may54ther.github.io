  "spellcheck": false,
  "propertiesInDocument": "source",
  "userIgnoreFilters": [
    "node_modules/",
    "attachments/",
    "*.js",
    "/node_modules//"
  ],
  "autoPairMarkdown": false,
  "autoPairBrackets": true,
  "smartIndentList": false
}